    background-color: #111;
    color: white;
    font-family: 'Courier New', Courier, monospace;

  }



  /* 대시보드 메인 동그라미 스타일 */
.placeholders h4 { 
  margin-bottom: 0;
  color: white;

}

.placeholder { 
  margin-top: 70px;
}

.placeholder img {
  display: inline-block;
  border-radius: 50%;
}

.img1 {
  position:absolute; 
  top:80px; 
  left:380px;
}

.img2 {
  position:absolute; 
  top:80px; 
  left:700px;
}

.img3 {
  position:absolute; 
  top:80px; 
  left:1000px;
}

/* 대시보드 동그라미 안에 글씨 넣는 스타일 */
.dash_wrap {
  position: relative;
}

.dash_image {
  vertical-align: middle;
}

.dash_text {
  top: 50%;
  left: 32%;
  position: absolute;
  transform: translate(-50%, -50%);
}

.content-box {
  /* float: middle; */
  position: absolute;
  top: 460px;
  width: 1050px;
  left: 300px;
}

